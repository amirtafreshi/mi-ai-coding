{
  "name": "mi-ai-coding",
  "version": "1.0.0",
  "description": "Production-ready, mobile-responsive AI Coding Platform with Next.js 15, Refine, Ant Design, Prisma ORM, and dual VNC integration for visual debugging and terminal access",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amirtafreshi/mi-ai-coding.git"
  },
  "bugs": {
    "url": "https://github.com/amirtafreshi/mi-ai-coding/issues"
  },
  "homepage": "https://github.com/amirtafreshi/mi-ai-coding#readme",
  "scripts": {
    "dev": "node server.js",
    "build": "prisma generate && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "refine": "refine",
    "test": "DISPLAY=:99 npx playwright test",
    "test:ui": "DISPLAY=:99 npx playwright test --ui",
    "test:headed": "DISPLAY=:99 npx playwright test --headed",
    "test:report": "DISPLAY=:99 npx playwright show-report",
    "test:vnc": "./scripts/test-vnc.sh"
  },
  "keywords": [
    "nextjs",
    "next.js",
    "react",
    "typescript",
    "refine",
    "refine-framework",
    "antd",
    "ant-design",
    "prisma",
    "prisma-orm",
    "postgresql",
    "vnc",
    "novnc",
    "vnc-viewer",
    "remote-desktop",
    "ai-coding",
    "coding-platform",
    "code-editor",
    "monaco-editor",
    "websocket",
    "real-time",
    "admin-panel",
    "dashboard",
    "playwright",
    "e2e-testing",
    "mobile-responsive",
    "fullstack",
    "multi-agent-system"
  ],
  "author": "MI AI Coding Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.16.3",
    "@refinedev/antd": "^6.0.2",
    "@refinedev/cli": "^2.16.49",
    "@refinedev/core": "^5.0.4",
    "@refinedev/nextjs-router": "^7.0.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@uiw/react-md-editor": "^4.0.4",
    "antd": "^5.27.4",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "monaco-editor": "^0.53.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "novnc-next": "^1.0.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^3.0.6",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/ws": "^8.18.1",
    "prisma": "^6.16.3",
    "ts-node": "^10.9.2"
  }
}
